public class EvenOrOdd1to100 
{
	public static void main(String args[])
	{
		
		
		System.out.println("Even numbers from 1 to 100:");
		for(int i=1;i<=100;i++)
		{
			if(i%2==0)
			{
				System.out.println(i);
			}
		}
		
		System.out.println("Odd numbers from 1 to 100:");
		for(int j=1;j<=100;j++)
		{
			if(j%2==1)
			{
				System.out.println(j);
			}
		}
	}
}
