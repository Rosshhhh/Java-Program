import java.util.Scanner;

public class Studentdetails 
{
	public static void main(String args[])
	{
		Scanner obj = new Scanner(System.in);
		
		System.out.println("Enter Student Roll.no:");
		int a=obj.nextInt();
		obj.nextLine();
		
		System.out.println("Enter Student Name:");
		String b=obj.nextLine();
		
		System.out.println("Enter the Tamil subject mark:");
		int s1=obj.nextInt();
		
		System.out.println("Enter the English subject mark:");
		int s2=obj.nextInt();
		
		System.out.println("Enter the Maths subject mark:");
		int s3=obj.nextInt();
		
		System.out.println("Enter the Science subject mark:");
		int s4=obj.nextInt();
		
		System.out.println("Enter the Social subject mark:");
		int s5=obj.nextInt();
		
		int avg=(s1+s2+s3+s4+s5)/5;
		
		System.out.println("Student Roll no is: " + a);
		System.out.println("Student Name is: " + b);
		System.out.println("Student's Tamil Mark is: " + s1);
		System.out.println("Student's English Mark is: " + s2);
		System.out.println("Student's Maths Mark is: " + s3);
		System.out.println("Student's Science Mark is: " + s4);
		System.out.println("Student's Social Mark is: " + s5);
		System.out.println("Student's overall average is: " + avg+"%");
	}
}
